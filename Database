CREATE TABLE ServiceContracts
(contractId varchar(20) NOT NULL,
serviceId varchar(20) NOT NULL)
go
ALTER TABLE ServiceContracts ADD CONSTRAINT Ma PRIMARY KEY (contractId, serviceId)
go
ALTER TABLE ServiceContracts
ADD CONSTRAINT fk_ten
FOREIGN KEY (contractId)
REFERENCES Contracts(contractId);
go

ALTER TABLE ServiceContracts
ADD CONSTRAINT fk_ten2
  FOREIGN KEY (serviceId)
  REFERENCES Services(serviceId);
